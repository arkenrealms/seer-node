{
  "name": "rune-databaser",
  "version": "2.0.0",
  "main": "build/src/index.js",
  "scripts": {
    "dev": "RUNE_ENV=local CUBE_BRIDGE_PORT=7777 ts-node-dev --quiet --respawn ./src/index.ts",
    "build": "NODE_ENV=production tsc",
    "start": "NODE_ENV=production node --max_semi_space_size=30000 --max-old-space-size=30000 --initial-old-space-size=8000 --optimize-for-size --experimental-modules --experimental-json-modules build/src/index.js",
    "forever": "forever start forever-config.json",
    "clean": "rm forever-script-err.log && rm forever-script-log.log && rm forever-script-out.log && rm nohup.out",
    "migrate-all": "ts-node -P ./tsconfig.json ./node_modules/.bin/knex migrate:latest --knexfile knexfile.js",
    "seed-all": "ts-node -P ./tsconfig.json ~/.nvm/versions/node/v14.16.0/bin/knex seed:run --knexfile knexfile.js",
    "seed": "ts-node -P ./tsconfig.json ~/.nvm/versions/node/v14.16.0/bin/knex seed:run --knexfile knexfile.js --specific=",
    "knex": "ts-node -P ./tsconfig.json ./node_modules/.bin/knex",
    "refresh-db": "yarn migrate-all && yarn seed-all"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.7",
    "@nomiclabs/buidler-ethers": "^1.3.4",
    "@nomiclabs/buidler-etherscan": "^2.1.0",
    "@nomiclabs/buidler-waffle": "^1.3.5",
    "@openzeppelin/cli": "^2.8.2",
    "@openzeppelin/contract-loader": "^0.6.1",
    "@openzeppelin/contracts": "^3.1.0-solc-0.7",
    "@openzeppelin/test-environment": "^0.1.4",
    "@openzeppelin/test-helpers": "^0.5.9",
    "@truffle/hdwallet-provider": "^1.2.0",
    "@typechain/ethers-v4": "^1.0.0",
    "@types/common-tags": "^1.8.0",
    "@types/cors": "^2.8.5",
    "@types/ethereum-protocol": "^1.0.0",
    "@types/express": "^4.16.1",
    "@types/express-rate-limit": "^3.3.0",
    "@types/helmet": "^0.0.43",
    "@types/morgan": "^1.7.35",
    "@types/node": "^13.7.0",
    "@types/web3": "1.0.20",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "@uniswap/lib": "^1.1.4",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "canonical-weth": "^1.4.0",
    "chai": "^4.2.0",
    "knex": "^0.21.1",
    "objection": "^2.1.3",
    "pg": "^8.0.3",
    "knex-migrate": "^1.7.0",
    "coingecko-api": "^1.0.10",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "^2.3.2",
    "ethers": "^5.1.4",
    "fs-jetpack": "^4.1.0",
    "ganache-cli": "^6.9.1",
    "mocha": "^8.1.1",
    "node-fetch": "^2.6.1",
    "prettier": "^2.0.5",
    "prettier-plugin-solidity": "^1.0.0-alpha.52",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.2",
    "solhint": "^3.0.0",
    "ts-generator": "0.0.8",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.2",
    "typechain": "^2.0.0",
    "typescript": "^4.3.4",
    "web3": "^1.2.11"
  },
  "engines": {
    "node": "14.*.*"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "ipfs-http-client": "^50.1.2",
    "js-md5": "^0.7.3",
    "json-beautify": "^1.1.1",
    "scrypt": "^6.0.3",
    "shortid": "^2.2.16",
    "sqlite3": "^5.0.2"
  }
}
